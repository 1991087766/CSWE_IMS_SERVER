CREATE TABLE `db_customer_all` (
	`编号` CHAR(200) NULL DEFAULT NULL,
	`车牌号` CHAR(50) NULL DEFAULT '—',
	`车架号` CHAR(50) NULL DEFAULT '—',
	`客户名称` CHAR(50) NULL DEFAULT '—',
	`品牌` CHAR(50) NULL DEFAULT '—',
	`车型号` CHAR(50) NULL DEFAULT '—',
	`发动机号` CHAR(50) NULL DEFAULT '—',
	`固定电话` CHAR(50) NULL DEFAULT NULL,
	`手机` CHAR(50) NULL DEFAULT NULL,
	`身份证号` CHAR(50) NULL DEFAULT NULL,
	`座位数` INT(11) UNSIGNED NOT NULL DEFAULT '4',
	`商业险日期` BIGINT(20) UNSIGNED NULL DEFAULT '0',
	`交强险日期` BIGINT(20) UNSIGNED NULL DEFAULT '0',
	`登记日期` BIGINT(20) UNSIGNED NULL DEFAULT '0',
	`地址` CHAR(50) NULL DEFAULT NULL,
	`状态` CHAR(50) NOT NULL DEFAULT '未处理',
	`备注` CHAR(200) NULL DEFAULT NULL,
	`汽车价格` CHAR(200) NULL DEFAULT NULL,
	`初保` CHAR(200) NULL DEFAULT NULL,
	`客服` CHAR(50) NOT NULL DEFAULT '—',
	`更新时间` CHAR(50) NULL DEFAULT NULL,
	`投保公司` CHAR(50) NULL DEFAULT NULL,
	`客户类型` CHAR(50) NULL DEFAULT NULL
)
COMMENT='客户信息'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `db_department` (
	`编号` CHAR(50) NULL DEFAULT NULL,
	`姓名` CHAR(50) NULL DEFAULT NULL,
	`部门` CHAR(50) NULL DEFAULT NULL,
	`管理` CHAR(50) NULL DEFAULT NULL,
	`状态` CHAR(50) NULL DEFAULT NULL,
	`入职时间` CHAR(50) NULL DEFAULT NULL,
	`离职时间` CHAR(50) NOT NULL DEFAULT '—',
	`地址` CHAR(100) NULL DEFAULT NULL
)
COMMENT='部门管理'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `db_info` (
	`编号` CHAR(50) NULL DEFAULT NULL,
	`账号` CHAR(50) NULL DEFAULT NULL,
	`密码` CHAR(50) NULL DEFAULT NULL,
	`性别` CHAR(50) NULL DEFAULT NULL,
	`部门` CHAR(50) NULL DEFAULT NULL,
	`地址` CHAR(100) NULL DEFAULT NULL,
	`手机` CHAR(50) NULL DEFAULT NULL,
	`邮箱` CHAR(50) NULL DEFAULT NULL,
	`管理员` INT(11) NOT NULL DEFAULT '1',
	`姓名` CHAR(50) NULL DEFAULT NULL,
	`管理` CHAR(50) NOT NULL DEFAULT '无',
	`状态` CHAR(50) NOT NULL DEFAULT '在职',
	`入职时间` CHAR(50) NOT NULL DEFAULT '—',
	`离职时间` CHAR(50) NOT NULL DEFAULT '—'
)
COMMENT='用户信息'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;


CREATE TABLE `db_login` (
	`编号` CHAR(50) NULL DEFAULT NULL,
	`USERNAME` CHAR(50) NULL DEFAULT NULL,
	`ACCESS_TOKEN` CHAR(50) NULL DEFAULT NULL,
	`IP` CHAR(50) NULL DEFAULT NULL
)
COMMENT='登录状态'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;


CREATE TABLE `db_log_check` (
	`编号` CHAR(50) NULL DEFAULT NULL,
	`账号` CHAR(50) NULL DEFAULT NULL,
	`部门` CHAR(50) NULL DEFAULT NULL,
	`IP` CHAR(50) NULL DEFAULT NULL,
	`时间` CHAR(50) NULL DEFAULT NULL,
	`操作` CHAR(50) NULL DEFAULT NULL
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;


CREATE TABLE `db_log_login` (
	`编号` CHAR(50) NULL DEFAULT NULL,
	`账号` CHAR(50) NULL DEFAULT NULL,
	`IP` CHAR(50) NULL DEFAULT NULL,
	`状态` CHAR(50) NULL DEFAULT NULL
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;


CREATE TABLE `db_new` (
	`编号` CHAR(50) NULL DEFAULT NULL,
	`来源` CHAR(50) NULL DEFAULT NULL,
	`主题` CHAR(50) NULL DEFAULT NULL,
	`状态` CHAR(50) NULL DEFAULT NULL,
	`内容` CHAR(200) NULL DEFAULT NULL
)
COMMENT='消息'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;

CREATE TABLE `del_customer` (
	`编号` CHAR(200) NULL DEFAULT NULL,
	`车牌号` CHAR(50) NULL DEFAULT NULL,
	`车架号` CHAR(50) NULL DEFAULT NULL,
	`客户名称` CHAR(50) NULL DEFAULT NULL,
	`品牌` CHAR(50) NULL DEFAULT NULL,
	`车型号` CHAR(50) NULL DEFAULT NULL,
	`发动机号` CHAR(50) NULL DEFAULT NULL,
	`固定电话` CHAR(50) NULL DEFAULT NULL,
	`手机` CHAR(50) NULL DEFAULT NULL,
	`身份证号` CHAR(50) NULL DEFAULT NULL,
	`座位数` INT(11) UNSIGNED NULL DEFAULT '0',
	`商业险日期` BIGINT(20) UNSIGNED NULL DEFAULT '0',
	`交强险日期` BIGINT(20) UNSIGNED NULL DEFAULT '0',
	`登记日期` BIGINT(20) UNSIGNED NULL DEFAULT '0',
	`地址` CHAR(50) NULL DEFAULT NULL,
	`状态` CHAR(50) NOT NULL DEFAULT '未处理',
	`备注` CHAR(200) NULL DEFAULT NULL,
	`汽车价格` CHAR(200) NULL DEFAULT NULL,
	`初保` CHAR(200) NULL DEFAULT NULL,
	`客服` CHAR(50) NOT NULL DEFAULT '客服',
	`更新时间` CHAR(50) NULL DEFAULT NULL,
	`投保公司` CHAR(50) NOT NULL DEFAULT '无',
	`客户类型` CHAR(50) NOT NULL DEFAULT '无'
)
COMMENT='客户信息'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;


CREATE TABLE `del_info` (
	`编号` CHAR(50) NULL DEFAULT NULL,
	`账号` CHAR(50) NULL DEFAULT NULL,
	`密码` CHAR(50) NULL DEFAULT NULL,
	`性别` CHAR(4) NULL DEFAULT NULL,
	`部门` CHAR(50) NULL DEFAULT NULL,
	`地址` CHAR(100) NULL DEFAULT NULL,
	`手机` CHAR(50) NULL DEFAULT NULL,
	`邮件` CHAR(50) NULL DEFAULT NULL,
	`管理员` INT(11) NOT NULL DEFAULT '1',
	`姓名` CHAR(50) NULL DEFAULT NULL
)
COMMENT='用户信息'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;


CREATE TABLE `del_new` (
	`编号` CHAR(50) NULL DEFAULT NULL,
	`来源` CHAR(50) NULL DEFAULT NULL,
	`主题` CHAR(50) NULL DEFAULT NULL,
	`状态` CHAR(50) NULL DEFAULT NULL,
	`内容` CHAR(200) NULL DEFAULT NULL
)
COMMENT='消息'
COLLATE='utf8_general_ci'
ENGINE=InnoDB
;